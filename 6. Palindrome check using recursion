#include <stdio.h>
#include <string.h>
int isPalindrome(char s[], int i, int j) {
    if (i >= j) return 1;
    if (s[i] != s[j]) return 0;
    return isPalindrome(s, i + 1, j - 1);
}
int main() {
    char str[] = "madam";
    if (isPalindrome(str, 0, strlen(str) - 1))
        printf("Palindrome");
    else
        printf("Not Palindrome");
    return 0;
}
